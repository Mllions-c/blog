{
  "extends": "standard",
  "env": {
    "node": true,
    "mocha": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react"],
  "globals": {
    "assert": true,
    "artifacts": true,
    "contract": true
  },
  "rules": {

  }
}
